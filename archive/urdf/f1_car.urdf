<?xml version="1.0"?>
<robot name="origins">

  <link name="base_link">
    <visual>
      <geometry>
        <!-- <mesh filename="package://robotrendszerek_projekt/meshes/f1_chassis.dae"/> -->
      </geometry>
	  <origin rpy="0 0 0" xyz="0.38 0 0.55" />
    </visual>
  </link>

  <link name="right_front_wheel">
    <visual>
      <geometry>
		<!-- <mesh filename="package://robotrendszerek_projekt/meshes/f1_wheel.dae"/> -->
        </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <joint name="right_front_wheel_joint" type="continuous">
	<axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin xyz="0.79 -0.25 0.14"/>
  </joint>

  <link name="left_front_wheel">
    <visual>
      <geometry>
		<!-- <mesh filename="package://robotrendszerek_projekt/meshes/f1_wheel.dae"/> -->
        </geometry>
      <origin rpy="3.14159 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <joint name="left_front_wheel_joint" type="continuous">
	<axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin xyz="0.79 0.25 0.14"/>
  </joint>

  <link name="right_rear_wheel">
    <visual>
      <geometry>
	<!-- <mesh filename="package://robotrendszerek_projekt/meshes/f1_wheel.dae"/> -->
        </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <joint name="right_rear_wheel_joint" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="right_rear_wheel"/>
    <origin xyz="0 -0.25 0.14"/>
  </joint>

  <link name="left_rear_wheel">
    <visual>
      <geometry>
	<!-- <mesh filename="package://robotrendszerek_projekt/meshes/f1_wheel.dae"/> -->
        </geometry>
      <origin rpy="3.14159 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <joint name="left_rear_wheel_joint" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="left_rear_wheel"/>
    <origin xyz="0 0.25 0.14"/>
  </joint>

</robot>