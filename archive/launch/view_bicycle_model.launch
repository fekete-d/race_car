<?xml version="1.0"?>
<!-- Display the bicycle_bot model in rviz.
-->
<launch>
    <!-- Pass the bicycle_bot model to view_model.launch  -->
    <include file="$(find robotrendszerek_projekt)/launch/view_model.launch">
        <arg name="model" value="$(find robotrendszerek_projekt)/urdf/bicycle.xacro" />
    </include>
</launch>